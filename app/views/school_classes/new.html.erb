<h3>New SchoolClass Page with Form</h3>

<h3>School Class Form</h3>

<%= form_for @school_class do |f| %>
    <p><label>Class Title: </label><br>
    <%= f.text_field :title %><br></p>

    <p><label>Room Number:  </label><br>
    <%= f.text_field :room_number %><br></p>

    <%= f.submit %>

<% end %>